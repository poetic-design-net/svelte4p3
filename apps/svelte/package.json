{
  "name": "svelte",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "dotenvx run --convention=nextjs -- cross-env NODE_OPTIONS=\"--no-deprecation --inspect\" vite dev",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepare": "svelte-kit sync",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@local/prettier-config",
  "dependencies": {
    "swiper": "^11.1.15"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.31.3",
    "@iconify/json": "^2.2.289",
    "@local/env": "workspace:*",
    "@local/eslint-config": "workspace:*",
    "@local/payload": "workspace:*",
    "@local/prettier-config": "workspace:*",
    "@local/tailwind-config": "workspace:*",
    "@local/tsconfig": "workspace:*",
    "@local/types": "workspace:*",
    "@local/ui": "workspace:*",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-node": "^5.2.11",
    "@sveltejs/kit": "^1.30.3",
    "@sveltejs/vite-plugin-svelte": "^2.4.3",
    "@types/eslint": "^9.6.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "payload": "^3.11.0",
    "prettier": "^3.4.2",
    "svelte": "^4.0.0",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "unplugin-icons": "^0.22.0",
    "vite": "^5.4.11"
  }
}
