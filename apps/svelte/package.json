{
  "name": "svelte",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "dotenvx run --convention=nextjs -- cross-env NODE_OPTIONS=\"--no-deprecation --inspect\" vite dev",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepare": "svelte-kit sync",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@local/prettier-config",
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.23.0",
    "@local/env": "workspace:*",
    "@local/eslint-config": "workspace:*",
    "@local/payload": "workspace:*",
    "@local/prettier-config": "workspace:*",
    "@local/tailwind-config": "workspace:*",
    "@local/tsconfig": "workspace:*",
    "@local/types": "workspace:*",
    "@local/ui": "workspace:*",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/kit": "^2.8.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.1",
    "@types/eslint": "^9.6.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "payload": "3.0.0-beta.130",
    "prettier": "^3.3.3",
    "svelte": "^5.2.0",
    "svelte-check": "^4.0.8",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  }
}
